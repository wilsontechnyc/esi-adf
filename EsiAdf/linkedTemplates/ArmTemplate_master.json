{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "EsiAdf"
		},
		"AzureTableStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureTableStorage1'"
		},
		"AzureBlobStorage1_properties_typeProperties_serviceEndpoint": {
			"type": "string",
			"defaultValue": "https://esiadf.blob.core.windows.net"
		},
		"AzureBlobStorageEsiUnivConst_properties_typeProperties_serviceEndpoint": {
			"type": "string",
			"defaultValue": "https://esiadf.blob.core.windows.net"
		},
		"AzureBlobStorageIncursions_properties_typeProperties_serviceEndpoint": {
			"type": "string",
			"defaultValue": "https://esiadf.blob.core.windows.net"
		},
		"AzureTableStorageEsiUniverseConstellations_sasUri": {
			"type": "secureString",
			"metadata": "Secure string for 'sasUri' of 'AzureTableStorageEsiUniverseConstellations'"
		},
		"EsiRegionInfo_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://esi.evetech.net/latest/universe/regions/?datasource=tranquility"
		},
		"EsiUniverseConstellations_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://esi.evetech.net/latest/universe/constellations/?datasource=tranquility"
		},
		"SystemKillsStorage_properties_typeProperties_serviceEndpoint": {
			"type": "string",
			"defaultValue": "https://esiadf.blob.core.windows.net"
		},
		"UniverseSystemJumps_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://esi.evetech.net/latest/universe/system_jumps/?datasource=tranquility"
		},
		"esiUnivConst_sasUri": {
			"type": "secureString",
			"metadata": "Secure string for 'sasUri' of 'esiUnivConst'"
		},
		"incursions_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://esi.evetech.net/latest/incursions/?datasource=tranquility"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "EsiAdf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureTableStorage1_connectionString": {
						"value": "[parameters('AzureTableStorage1_connectionString')]"
					},
					"AzureBlobStorage1_properties_typeProperties_serviceEndpoint": {
						"value": "[parameters('AzureBlobStorage1_properties_typeProperties_serviceEndpoint')]"
					},
					"AzureBlobStorageEsiUnivConst_properties_typeProperties_serviceEndpoint": {
						"value": "[parameters('AzureBlobStorageEsiUnivConst_properties_typeProperties_serviceEndpoint')]"
					},
					"AzureBlobStorageIncursions_properties_typeProperties_serviceEndpoint": {
						"value": "[parameters('AzureBlobStorageIncursions_properties_typeProperties_serviceEndpoint')]"
					},
					"AzureTableStorageEsiUniverseConstellations_sasUri": {
						"value": "[parameters('AzureTableStorageEsiUniverseConstellations_sasUri')]"
					},
					"EsiRegionInfo_properties_typeProperties_url": {
						"value": "[parameters('EsiRegionInfo_properties_typeProperties_url')]"
					},
					"EsiUniverseConstellations_properties_typeProperties_url": {
						"value": "[parameters('EsiUniverseConstellations_properties_typeProperties_url')]"
					},
					"SystemKillsStorage_properties_typeProperties_serviceEndpoint": {
						"value": "[parameters('SystemKillsStorage_properties_typeProperties_serviceEndpoint')]"
					},
					"UniverseSystemJumps_properties_typeProperties_url": {
						"value": "[parameters('UniverseSystemJumps_properties_typeProperties_url')]"
					},
					"esiUnivConst_sasUri": {
						"value": "[parameters('esiUnivConst_sasUri')]"
					},
					"incursions_properties_typeProperties_url": {
						"value": "[parameters('incursions_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "EsiAdf_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'EsiAdf_ArmTemplate_0')]"
			]
		}
	]
}